try:
  a = int(input("Masukkan bilangan pertama: "))
  b = int(input("Masukkan bilangan kedua: "))
  c = int(input("Masukkan bilangan ketiga: "))
except ValueError:
  print("Input tidak valid. Masukkan angka bulat.")
else:
  # Menentukan bilangan terbesar
  if a > b and a > c:
    print("Terbesar: ", a)
  elif b > a and b > c:
    print("Terbesar: ", b)
  elif c > a and c > b:
    print("Terbesar: ", c)
