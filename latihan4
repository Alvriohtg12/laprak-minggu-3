try:
    sisi1 = int(input("Masukkan bilangan pertama: "))
    sisi2 = int(input("Masukkan bilangan kedua: "))
    sisi3 = int(input("Masukkan bilangan ketiga: "))
except ValueError:
  print("Input tidak valid.")
else:
   if sisi1 == sisi2 and sisi2 == sisi3:
        print("3 sisi sama")
   elif sisi1 == sisi2 or sisi1 == sisi3 or sisi2 == sisi3:
        print("2 sisi sama")
   else:
        print("Tidak ada yang sama")
